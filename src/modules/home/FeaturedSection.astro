---
import Container from "@components/Container/Container.astro";
import SectionFull from "@components/Section/SectionFull/SectionFull.astro";
import { Image } from "astro:assets";
import logo1 from "./assets/logoipsum-218.svg";
import logo2 from "./assets/logoipsum-239.svg";
import logo3 from "./assets/logoipsum-286.svg";
import logo4 from "./assets/logoipsum-263.svg";
import logo5 from "./assets/logoipsum-284.svg";
---

<SectionFull class="bg-white">
  <Container class="flex justify-between gap-7 overflow-x-scroll">
    <Image src={logo1} alt="The Guardian" />
    <Image src={logo2} alt="Homes & Gardens" />
    <Image src={logo3} alt="Ideal Home" />
    <Image src={logo4} alt="Elle Decor" />
    <Image src={logo5} alt="Vogue" />
  </Container>
</SectionFull>
